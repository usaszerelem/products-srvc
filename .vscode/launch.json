            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/src/index.ts",
            "outFiles": ["${workspaceFolder}/**/*.js"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Products",
            "outputCapture": "std",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/src/scripts/createProducts.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": ["${workspaceFolder}/**/*.js"]
        }
    ]
}
